# /init.kud.rc - Part of KudKernel

on init
    write /sys/block/zram0/comp_algorithm lz4
    write /sys/block/zram0/max_comp_streams 4
    write /sys/block/zram0/disksize 536870912

    write /proc/sys/vm/page-cluster 0

on late-fs
    # Set BFQs slice_idle to 0
    write /sys/block/mmcblk0/queue/iosched/slice_idle 0
    write /sys/block/mmcblk1/queue/iosched/slice_idle 0
    write /sys/block/dm-0/queue/iosched/slice_idle 0
    write /sys/block/dm-1/queue/iosched/slice_idle 0
    write /sys/block/sda/queue/iosched/slice_idle 0
    write /sys/block/sda/queue/read_ahead_kb 2048
    write /sys/block/sda/queue/nr_requests 256
    write /sys/block/dm-0/queue/read_ahead_kb 2048
    write /sys/block/dm-1/queue/read_ahead_kb 2048

on post-fs
    exec u:r:init:s0 -- /system/bin/mkswap /dev/block/zram0
    exec u:r:init:s0 -- /system/bin/swapon /dev/block/zram0

# Run post-boot script after Qualcomms post_boot shell script
on property:init.svc.qcom-post-boot=stopped


    # I/O
    write /sys/block/sda/queue/read_ahead_kb 128
    write /sys/block/sda/queue/nr_requests 128
    write /sys/block/dm-0/queue/read_ahead_kb 128
    write /sys/block/dm-1/queue/read_ahead_kb 128

    write /proc/sys/vm/swappiness 100
    write /sys/module/lowmemorykiller/parameters/minfree "18432,23040,27648,32256,36864,46080"

    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "schedutil"

    # If the selected governor is valid, apply tunables
    setprop sys.cpufreq.governor ${persist.sys.cpufreq.governor}

    # TEST
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1689000

    # Set read ahead to 128 kb for external storage (rest are handled by qcom-post-boot)
    write /sys/block/mmcblk1/queue/read_ahead_kb 128

# schedutil CPUFreq governor
on property:sys.cpufreq.governor=schedutil
    # Apply (most of) interactive tunables to clarity
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor schedutil
    write /sys/devices/system/cpu/cpufreq/schedutil/up_rate_limit_us 350
    write /sys/devices/system/cpu/cpufreq/schedutil/down_rate_limit_us 1500
    write /sys/devices/system/cpu/cpufreq/schedutil/iowait_boost_enable 1

    # Enable input boost configuration
    write /sys/module/cpu_boost/parameters/input_boost_freq "0:1036800"
    write /sys/module/cpu_boost/parameters/dynamic_stune_boost 15
    write /sys/module/cpu_boost/parameters/input_boost_ms 100

    # set default schedTune value for foreground/top-app (only affects EAS)
    write /dev/stune/foreground/schedtune.prefer_idle 1
    write /dev/stune/top-app/schedtune.prefer_idle 1
    write /dev/stune/top-app/schedtune.boost 1
    write /dev/stune/rt/schedtune.prefer_idle 1
